PORT=3000
VAPID_CONTACT_EMAIL=mailto:admin@example.com

# Option 1 (recommended): set keys separately
VAPID_PUBLIC_KEY=replace_with_public_key
VAPID_PRIVATE_KEY=replace_with_private_key

# Option 2: set both keys in one env var
# VAPID_KEYS={"publicKey":"replace_with_public_key","privateKey":"replace_with_private_key"}
# or
# VAPID_KEYS=replace_with_public_key:replace_with_private_key

MAX_SEND_COUNT=20
DEFAULT_INTERVAL_MS=1000

